<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Time_Zone__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISBLANK($User.State), &quot;None&quot;,
IF(CONTAINS(&quot;AK:AZ:CA:HA:NV:NM:OR:UT:WA&quot;, $User.State), &quot;West&quot;,
IF(CONTAINS(&quot;CO:ID:MT:KS:OK:TX:WY&quot;, $User.State), &quot;Central&quot;,
IF(CONTAINS(&quot;CT:ME:MA:NH:NY:PA:RI:VT&quot;, $User.State), &quot;East&quot;,
IF(CONTAINS(&quot;AL:AR:DC:DE:FL:GA:KY:LA:MD:MS:NC:NJ:SC:TN:VA:WV&quot;,
$User.State), &quot;South&quot;,
IF(CONTAINS(&quot;IL:IN:IA:MI:MN:MO:NE:ND:OH:SD:WI&quot;, $User.State), &quot;North&quot;,
&quot;Other&quot;))))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Time Zone</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
