<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HRCSVC_Total_Price__c</fullName>
    <description>Field that calculates the total price of a product consumed line by multiplying the quantity consumed and unit price. SE-195</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(HRCSVC_Unit_Price_Override__c),
(100 - HRCSVC_Discount_Percent__c) * (UnitPrice * QuantityConsumed),
QuantityConsumed * HRCSVC_Unit_Price_Override__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Price</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
