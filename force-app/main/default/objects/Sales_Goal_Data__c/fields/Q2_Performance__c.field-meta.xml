<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Q2_Performance__c</fullName>
    <description>Indicates the total amount of Q2 sales for comparison against the quarterly goal.</description>
    <externalId>false</externalId>
    <formula>Q2_OPEN__c +  Q2_SHIPPED__c +  Q2_INVOICED_RAW__c +  Q2_ADJUSTMENT__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Q2 Performance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
