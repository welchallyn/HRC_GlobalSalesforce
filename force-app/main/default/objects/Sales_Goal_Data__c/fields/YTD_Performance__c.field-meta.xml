<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>YTD_Performance__c</fullName>
    <description>Indicates the total amount of YTD sales for comparison against the quarterly goal.</description>
    <externalId>false</externalId>
    <formula>YTD_OPEN__c +  YTD_SHIPPED__c +  YTD_INVOICED_RAW__c +  YTD_ADJUSTMENT__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>YTD Performance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
