<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Q2_Percent_to_Quota__c</fullName>
    <description>Indicates the percentage of Q2 sales relative to the goal for the quarter.</description>
    <externalId>false</externalId>
    <formula>IF( Q2_Performance__c = 0, 0, Q2_Performance__c /Q2_GOAL__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Q2 % to Quota</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
