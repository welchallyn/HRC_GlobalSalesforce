<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Q1_Performance__c</fullName>
    <description>Indicates the total amount of Q1 sales for comparison against the quarterly goal.</description>
    <externalId>false</externalId>
    <formula>Q1_OPEN__c +  Q1_SHIPPED__c +  Q1_INVOICED_RAW__c +  Q1_ADJUSTMENT__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Q1 Performance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
