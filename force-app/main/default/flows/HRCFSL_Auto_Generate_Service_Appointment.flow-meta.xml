            </conditions>
            <connector>
                <targetReference>Check_the_WO_Duration</targetReference>
            </connector>
            <label>WO is Tech Generated</label>
        </rules>
        <rules>
            <name>WO_is_System_Generated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>WorkOrderRec.HRCFSL_Tech_Generated__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WorkOrderRec.HRCFSL_Is_Kit_Child__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WorkOrderRec.HRCSVC_Remote_Tech_Support__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Service_Appointment_System</targetReference>
            </connector>
            <label>WO is System Generated</label>
        </rules>
