    <decisions>
        <description>Check the WO Duration field.</description>
        <name>Check_the_WO_Duration</name>
        <label>Check the WO Duration</label>
        <locationX>248</locationX>
        <locationY>1070</locationY>
        <defaultConnector>
            <targetReference>Update_SA_Actual_End_Date</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>WO Duration is not blank</defaultConnectorLabel>
        <rules>
            <name>WO_Duration_is_Blank_or_Zero</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>WorkOrderRec.DurationInMinutes</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WorkOrderRec.DurationInMinutes</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Location</targetReference>
            </connector>
            <label>WO Duration is Blank or Zero</label>
        </rules>
    </decisions>
