        <locationX>677</locationX>
        <locationY>950</locationY>
        <defaultConnectorLabel>No-op</defaultConnectorLabel>
        <rules>
            <name>WO_is_Tech_Generated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>WorkOrderRec.HRCFSL_Tech_Generated__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>WorkOrderRec.HRCSVC_Remote_Tech_Support__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
