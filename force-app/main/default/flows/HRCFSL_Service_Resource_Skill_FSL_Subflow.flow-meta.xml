            </connector>
            <label>Active skill present</label>
        </rules>
    </decisions>
    <decisions>
        <description>check whether the skill expired or current</description>
        <name>Is_Skill_Expired</name>
        <label>Is Skill Expired?</label>
        <locationX>932</locationX>
        <locationY>277</locationY>
        <defaultConnector>
            <targetReference>Loop_SR_Skills</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Skill Expired</defaultConnectorLabel>
        <rules>
            <name>Skill_is_Current</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SRSkill_var.EffectiveEndDate</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SRSkill_var.EffectiveEndDate</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>$Flow.CurrentDateTime</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_SR_Skills</targetReference>
            </connector>
            <label>Skill is Current</label>
        </rules>
    </decisions>
    <decisions>
        <description>Service Resource has Active skills</description>
        <name>Service_Resource_has_skills</name>
        <label>Service Resource has skills?</label>
        <locationX>797</locationX>
        <locationY>50</locationY>
        <defaultConnectorLabel>No Skill</defaultConnectorLabel>
        <rules>
            <name>Has_Skill</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>SRSkiils_list</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Loop_SR_Skills</targetReference>
            </connector>
            <label>Has Skill</label>
        </rules>
    </decisions>
    <decisions>
        <description>WO Skill Matches with SR Skills</description>
        <name>Skill_Matches_with_SR_Skills</name>
        <label>Skill Matches with SR Skills</label>
        <locationX>1104</locationX>
        <locationY>382</locationY>
        <defaultConnector>
            <targetReference>Loop_WO_Skills</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Skill doesn&apos;t Match</defaultConnectorLabel>
        <rules>
            <name>Skill_Match</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SR_Skills_var</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <elementReference>WO_Skill_var.SkillId</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_SR_hasSkill_Flag</targetReference>
            </connector>
            <label>Skill Match</label>
        </rules>
    </decisions>
    <decisions>
        <description>Skills Requirement Exist for Work order</description>
        <name>WO_Skills_Requirement_Exist</name>
        <label>Skills Requirement Exist?</label>
        <locationX>326</locationX>
        <locationY>52</locationY>
        <defaultConnector>
            <targetReference>Update_Has_skill_attribute</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>WO doesn&apos;t have Skill requirement</defaultConnectorLabel>
        <rules>
            <name>Skills_Requirement_Exist</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>WOReqSkill_list</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Service_Reource</targetReference>
            </connector>
            <label>Skills Requirement Exist</label>
        </rules>
    </decisions>
    <interviewLabel>HRCFSL Service Resource Skill FSL Subflow {!$Flow.CurrentDateTime}</interviewLabel>
