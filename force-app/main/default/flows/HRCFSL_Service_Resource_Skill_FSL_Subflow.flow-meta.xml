    <decisions>
        <description>Active Skill Present</description>
        <name>Active_Skill_Present</name>
        <label>Active Skill Present?</label>
        <locationX>1091</locationX>
        <locationY>52</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Is_Active_skill_present</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>SR_Skills_var</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>SR_Skills_var</leftValueReference>
