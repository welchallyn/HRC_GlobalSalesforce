    <decisions>
        <name>Check_Return_type_for_ROLI</name>
        <label>Check Return type for ROLI</label>
        <locationX>446</locationX>
        <locationY>278</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Return_Type_is_Exchange</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ROLIRecord.HRCSVC_Return_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Exchange</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Work_Order_for_Exchange_Return_Type</targetReference>
            </connector>
            <label>Return Type is Exchange</label>
        </rules>
        <rules>
            <name>Return_Type_is_Credit_Return</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ROLIRecord.HRCSVC_Return_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Credit Return</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Work_Order_for_Credit_Return_Type</targetReference>
            </connector>
            <label>Return Type is Credit Return</label>
        </rules>
        <rules>
            <name>Return_Type_is_Repair</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ROLIRecord.HRCSVC_Return_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Repair</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Work_Order_for_Repair_Return_Type</targetReference>
            </connector>
            <label>Return Type is Repair</label>
        </rules>
    </decisions>
    <decisions>
        <name>Check_ROLI_is_null_or_not</name>
        <label>Check ROLI is null or not</label>
        <locationX>776</locationX>
        <locationY>158</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Work_Order_is_not_null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ROLIRecord.Id</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_Return_type_for_ROLI</targetReference>
            </connector>
            <label>ROLI is not null</label>
        </rules>
    </decisions>
