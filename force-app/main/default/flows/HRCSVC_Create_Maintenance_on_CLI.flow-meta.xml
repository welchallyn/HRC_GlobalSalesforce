    <formulas>
        <name>Ent_Start_Date</name>
        <dataType>Date</dataType>
        <expression>IF({!Loop_thru_Ents.StartDate} &lt; {!ContractLineItemRecord.HRCSVC_Cancel_Date__c},{!Loop_thru_Ents.StartDate},{!ContractLineItemRecord.HRCSVC_Cancel_Date__c} - 1)</expression>
    </formulas>
    <formulas>
        <name>FirstWODueDate</name>
        <dataType>Date</dataType>
        <expression>IF(ADDMONTHS({!ContractLineItemRecord.StartDate}, {!IterateMaintenancePlan.HRCSVC_Offset__c}) &lt; TODAY() + 1, TODAY() + 1, ADDMONTHS({!ContractLineItemRecord.StartDate}, {!IterateMaintenancePlan.HRCSVC_Offset__c}))</expression>
    </formulas>
    <formulas>
        <name>ModifiedStartDate</name>
        <dataType>Date</dataType>
        <expression>IF(ADDMONTHS({!ChangedStartDate}, {!MaintenanceAssetRecord.MaintenancePlan.HRCSVC_Maintenance_Plan_Template__r.HRCSVC_Offset__c}) &lt; TODAY() + 1, TODAY() + 1, ADDMONTHS({!ChangedStartDate}, {!MaintenanceAssetRecord.MaintenancePlan.HRCSVC_Maintenance_Plan_Template__r.HRCSVC_Offset__c}))</expression>
    </formulas>
