   LastModified Date: 16th Feb,2017
   Last Modified By: Jenish Shingala.
   Description: Visualforce page for print view of strategic deal review information.
   -->

<apex:page standardController="Opportunity" extensions="StrategicDealInfoController">
   <style>
      .slds-col{
      width:100% !important;
      }
      
      .slds-checkbox--faux{
      border:1px solid black !important;
      }
      .slds-form-element__label{
      font-weight: bold; 
      font-family: sans-serif;
      }
      /*::-webkit-input-placeholder { line-height:normal;  opacity: 0.5 !important; color:red !important;}*/
      
     
      label{
      white-space: nowrap;
      }
      
   </style>
   <script></script>
   <h4 style="font-size:150%;font-family: initial;">Strategic Deal Review for {!Opportunity.name}</h4>
   <br/><br/>
   <apex:form >
     
      <html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
         <head>
            <apex:stylesheet value="{!URLFOR($Resource.LightningUI, 'assets/styles/salesforce-lightning-design-system-vf.css')}" />
            
         </head>
         <div class="slds">
            <div class="container">
               <article class="slds-card">
                  <div class="slds-card__body" id="formdiv">
                     <div class="slds-grid">
                        <div class="slds-col" style="margin-left:1%;">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="date1">
                                 Proposal Submission Date:
                              </label>
                              <div class="slds-form-element__control" style="width:150px;">
                               <apex:outputfield id="date1" styleclass="slds-input" value="{!objOpp.Tender_Due_Date__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="date2">
                                 Customer Decision Date:
                              </label>
                              <div class="slds-form-element__control" style="width:150px;">
                                <apex:outputfield id="date2" styleclass="slds-input" value="{!objOpp.Customer_Decision_Date__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="margin-left: 1%;">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input2">Maximum Tender Price:</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input2" styleclass="slds-input" value="{!objOpp.Maximum_Tender_Price__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="margin-left:5%;margin-right: 1%;">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="checkbox">Governance Support Needed:</label>
                              <label class="slds-checkbox" id="checkbox">
                                 <apex:outputfield styleclass="slds-input" value="{!objOpp.Governance_Support_Needed__c}" />
                                
                              </label>
                           </div>
                        </div>
                     </div><br/>
                     <div class="slds-grid" style="margin-top: 0.7%;">
                        <div class="slds-col" style="margin-left: 1%;">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input3">Price: Weight:</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input3" styleclass="slds-input" value="{!objOpp.Price_Weight__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="margin-left: 1%;margin-right: 1%">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input4">Price: HRC Rating:</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input4" styleclass="slds-input" value="{!objOpp.Price_HRC_Rating__c}"/>
                              </div>
                           </div>
                        </div>
                     </div>
                     <div class="slds-grid" style="margin-top: 0.7%;">
                        <div class="slds-col" style="margin-left: 1%">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input5">Specification: Weight:</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input5" styleclass="slds-input" value="{!objOpp.Specification_Weight__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="margin-left: 1%;margin-right: 1%">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input6">Specification: HRC Rating:</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input6" styleclass="slds-select" value="{!objOpp.Specification_HRC_Rating__c}"/>
                              </div>
                           </div>
                        </div>
                     </div>
                     <div class="slds-grid" style="margin-top: 0.7%;">
                        <div class="slds-col" style="margin-left: 1%;">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input7">Service: Weight:</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input7" styleclass="slds-select" value="{!objOpp.Service_Weight__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="margin-left: 1%;margin-right:1%">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input8">Service: HRC Rating</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input8" styleclass="slds-select" value="{!objOpp.Service_HRC_Rating__c}"/>
                              </div>
                           </div>
                        </div>
                     </div>
                     <div class="slds-grid" style="margin-top: 0.7%;">
                        <div class="slds-col" style="margin-left: 1%;">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input9">Relationship: Weight</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input9" styleclass="slds-select" value="{!objOpp.Relationship_Weight__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="margin-left: 1%;margin-right:1%">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input15">Relationship: HRC Rating</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input15" styleclass="slds-select" value="{!objOpp.Relationship_HRC_Rating__c}"/>
                              </div>
                           </div>
                        </div>
                     </div>
                     <div class="slds-grid" style="margin-top: 0.7%;">
                        <div class="slds-col" style="margin-left: 1%;">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input11">Quality: Weight</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input11" styleclass="slds-select" value="{!objOpp.Quality_Weight__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="margin-left: 1%;margin-right:1%">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input10">Quality: HRC Rating</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input10" styleclass="slds-select" value="{!objOpp.Quality_HRC_Rating__c}"/>
                              </div>
                           </div>
                        </div>
                     </div><br/>
                     <div class="slds-grid" style="margin-top: 0.7%;">
                        <div class="slds-col" style="margin-left: 1%;">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input12">SWOT Details (English):</label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input12" styleclass="slds-textarea" value="{!objOpp.SWOT_Details__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="margin-left: 1%;">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input13">
                                 Customer Specification (English):
                              </label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input13" styleclass="slds-textarea" value="{!objOpp.Customer_Specification__c}"/>
                              </div>
                           </div>
                        </div>
                        <div class="slds-col" style="margin-left: 1%;margin-right:1%">
                           <div class="slds-form-element">
                              <label class="slds-form-element__label" for="input14">
                                 Hill-Rom Proposal (English):
                              </label>
                              <div class="slds-form-element__control">
                                 <apex:outputfield id="input14" styleclass="slds-textarea" value="{!objOpp.Hill_Rom_Proposal__c}"/>
                              </div>
                           </div>
                        </div>
                     </div>
                     
                  </div>
               </article>
            </div>
         </div>
      </html>
   </apex:form>
   
</apex:page>